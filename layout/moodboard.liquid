<!doctype html>
<html class="no-js" lang="{{ request.locale.iso_code }}" dir="ltr">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width,initial-scale=1">
  <title>{{ 'moodboard.tipografias' | t }}</title>
  <link rel="stylesheet" href="{{ 'moodboard.css' | asset_url }}" />
  {{ content_for_header }}
</head>
<body>
  <main>
    {{ content_for_layout }}
    <section id="moodboard-main" class="custom-main">
        <div class="background fondo7" style="background-image: url('{{ 'regla.svg' | asset_url }}');"></div>
        <div class="background fondo4"></div>
        <div class="background fondo5"></div>
        <div class="background fondo6"></div>
        <div class="background fondo3"></div>
        <div class="page-width section-group ">
            <!-- contenedor del inicio del test -->
            <div id="moodboard-inicio"  class="home-hero bg-white">
                <div class="hero-content">
                    <img src="{{ 'logo-garys.svg' | asset_url }}"  height="43" width="130" alt="{LogoGarys}" />
                  <h6 class="hero-subtitle">{{ 'moodboard.testInteractivo' | t }}</h6>
                  <h1 class="hero-title">{{ 'moodboard.experienciaPersonalizada' | t }}</h1>
                  <p class="hero-description">{{ 'moodboard.descripcionMoodboard' | t }}</p>
                  <button onclick="startSteps()" class="btn btn--primary">{{ 'moodboard.comenzar' | t }}</button>
                </div>
            </div>
            <!-- contenedor del final de test-->
            <div id="moodboard-fin"  class="home-hero bg-white">
            <div class="hero-content">
                <img src="{{ 'logo-garys.svg' | asset_url }}"  height="43" width="130" alt="{LogoGarys}" />
                <h1 class="hero-title">{{ 'moodboard.graciasPorTest' | t }}</h1>
                <p class="hero-description">{{ 'moodboard.preferenciasRegistradas' | t }}</p>
                <button onclick="resetSteps()" class="btn btn--primary">{{ 'moodboard.volverInicio' | t }}</button>
            </div>
            </div>
        </div>
    </section>
    <section id="moodboard-content" class="custom-main">
        <div class="background fondo1"></div>
        <div class="background fondo2"></div>
        <div class="page-width section-group">
            <div class="section__header">
                <div class="header-logo gid-2">
                    <span class="text-center"> 
                        <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="111" height="37" viewBox="0 0 111 37" fill="none">
                            <rect width="111" height="37" fill="url(#pattern0_83_634)"/>
                            <defs>
                            <pattern id="pattern0_83_634" patternContentUnits="objectBoundingBox" width="1" height="1">
                            <use xlink:href="#image0_83_634" transform="scale(0.00613497 0.0185185)"/>
                            </pattern>
                            <image id="image0_83_634" width="163" height="70" preserveAspectRatio="none" xlink:href="data:image/png;base64,"/>
                            </defs>
                        </svg>
                    </span>
                    <button class="btn btn-close text-end" onclick="resetSteps();">
                        <svg xmlns="http://www.w3.org/2000/svg" width="35" height="35" viewBox="0 0 35 35" fill="none">
                            <path d="M19.0607 17.8536L26.7071 10.2071L26 9.5L18.3536 17.1464L10.7071 9.5L10 10.2071L17.6464 17.8536L10 25.5L10.7071 26.2071L18.3536 18.5607L26 26.2071L26.7071 25.5L19.0607 17.8536Z" fill="black"/>
                        </svg>
                    </button>
                </div>
                <div class="section__arrow gid-3 ">
                    <button id="arrow_previousStep" onclick="previousStep();" class="disabled">
                        <svg xmlns="http://www.w3.org/2000/svg" width="40" height="40" viewBox="0 0 40 40" fill="none">
                            <rect width="40" height="40" rx="20" fill="#F5F5F5"/>
                            <path d="M10.6667 20H29.3334" stroke="#434343" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                            <path d="M10.6667 20L18.6667 28" stroke="#434343" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                            <path d="M10.6667 20L18.6667 12" stroke="#434343" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                        </svg>
                    </button>
                    <div> <span id="indicador-pasos">1/7</span></div>
                    <button id="arrow_nextStep" onclick="nextStep();" class="disabled">
                        <svg xmlns="http://www.w3.org/2000/svg" width="40" height="40" viewBox="0 0 40 40" fill="none">
                            <rect x="40" y="40" width="40" height="40" rx="20" transform="rotate(180 40 40)" fill="#F5F5F5"/>
                            <path d="M29.3333 20L10.6666 20" stroke="#434343" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                            <path d="M29.3333 20L21.3333 12" stroke="#434343" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                            <path d="M29.3333 20L21.3333 28" stroke="#434343" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                        </svg>
                    </button>
                </div>
            </div>
    <!-- INICIO - CONTENIDO -->
