<script type="text/javascript">
    const btnSiguiente = document.querySelector('#btn_nextStep');
    const arrowSiguiente = document.querySelector('#arrow_nextStep');
    const arrowAnterior = document.querySelector('#arrow_previousStep');
    const contactoInputs = ['nombre', 'email'];

    const getCurrentStep = () => parseInt(sessionStorage.getItem('currentStep')) || 0;
    const setCurrentStep = (step) => sessionStorage.setItem('currentStep', step);
    const tipos = {
        2: 'imagen_proyectar',
        3: 'uniforme_estaciones',
        4: 'preferencia_materiales',
        5: 'estilo_marca',
        6: 'departamentos_uniformes',
        7: 'preferencias_estilo'
    };
    const clavesMoodboard = [
        'currentStep',
        'selectedPrincipalColors',
        'selectedSecundaryColors',
        'imagen_proyectar',
        'uniforme_estaciones',
        'preferencia_materiales',
        'estilo_marca',
        'departamentos_uniformes',
        'preferencias_estilo',
        'datos_contacto'
    ];

    function validateColorInput(e) {
        const input = e.target;
        const checkIcon = input.nextElementSibling;
        checkIcon.style.display =  input.value.trim() !== '' ? 'grid' : 'none';
    }

    function setActiveStep(stepNumber) {
        setCurrentStep(stepNumber);
        updateStepDisplay();
    }

    function updateStepDisplay() {
        const currentStep = getCurrentStep();
        const steps = document.querySelectorAll('.section__content');
        
        steps.forEach((step, index) => {
            step.classList.toggle('active', index + 1 === currentStep);
        });

        const indicador = document.getElementById('indicador-pasos');
        if (indicador) indicador.textContent = `${currentStep}/${steps.length}`;
        if(currentStep === 0){
            resetAll();
        }else if (currentStep === 1){
            arrowAnterior.classList.add('disabled');
            document.querySelector('#alertOpcionObligatoria').style.display = 'none';
            document.querySelector('#alertContactoObligatorio').style.display = 'none';
            document.querySelector('#alertColorObligatorio').style.display = 'inherit';
        }else if (currentStep === 8){
            document.querySelector('#alertOpcionObligatoria').style.display = 'none';
            document.querySelector('#alertColorObligatorio').style.display = 'none';
            document.querySelector('#alertContactoObligatorio').style.display = 'inherit';
        }else{
            arrowAnterior.classList.remove('disabled');
            document.querySelector('#alertColorObligatorio').style.display = 'none';
            document.querySelector('#alertContactoObligatorio').style.display = 'none';
            document.querySelector('#alertOpcionObligatoria').style.display = 'inherit';
        }
    }
    function resetAll(){
        btnSiguiente.disabled = true;
        arrowSiguiente.classList.add('disabled');
        arrowAnterior.classList.add('disabled');

        const container = document.querySelector('#moodboard-content');
        container.querySelectorAll('input[type="checkbox"]').forEach((checkbox) => {
                checkbox.checked = false;
                });
        container.querySelectorAll('input[type="text"], input[type="color"]').forEach((input) => {
            input.value = "";
        });
        container.querySelectorAll('.field__button').forEach((btn) => {
            btn.style.display = 'none';
        });
        container.querySelectorAll('.additional_input').forEach((add_input) => {
            add_input.remove();
        });
        clavesMoodboard.forEach(clave => sessionStorage.removeItem(clave));
    }

    function nextStep() {
        const currentStep = getCurrentStep();
        if (arrowSiguiente.classList.contains('disabled')) {
            btnSiguiente.scrollIntoView({ behavior: 'smooth' });
            return;
        }
        almacenarValores(currentStep);
        const nextStep = currentStep < 9 ? currentStep + 1 : 9;
        if (nextStep === 9){
            finallySteps();
        }else{
            setActiveStep(nextStep);
            actualizarBotonSiguiente();
        }
    }

    function previousStep() {
        const currentStep = getCurrentStep();
        const prevStep = currentStep > 1 ? currentStep - 1 : 1;
        setActiveStep(prevStep);
        verificarSeleccionYHabilitarBoton();
    }

    function startSteps(){
        setCurrentStep(1);
        const moodboardMain = document.querySelector('#moodboard-main');
        if (moodboardMain) {
            moodboardMain.style.display = 'none';
        }
        const moodboardContent = document.querySelector('#moodboard-content');
        if (moodboardContent) {
            moodboardContent.style.display = 'grid';
        }
        updateStepDisplay();
    }

    function resetSteps(){
        setCurrentStep(0);
        const moodboardMain = document.querySelector('#moodboard-main');
        if (moodboardMain) {
            moodboardMain.style.display = 'grid';
            const moodboardInicio = document.querySelector('#moodboard-inicio');
            if (moodboardInicio) moodboardInicio.style.display = 'grid';
            const moodboardFin = document.querySelector('#moodboard-fin');
            if (moodboardFin) moodboardFin.style.display = 'none';
        }
        const moodboardContent = document.querySelector('#moodboard-content');
        if (moodboardContent) {
            moodboardContent.style.display = 'none';
        }
        updateStepDisplay();
    }

    async function finallySteps(){

        const envioDatosOk = await enviarDatosPorCorreo();
        if (!envioDatosOk) return;

        const moodboardMain = document.querySelector('#moodboard-main');
        if (moodboardMain) {
            moodboardMain.style.display = 'grid';
            const moodboardInicio = document.querySelector('#moodboard-inicio');
            if (moodboardInicio) moodboardInicio.style.display = 'none';
            const moodboardFin = document.querySelector('#moodboard-fin');
            if (moodboardFin) moodboardFin.style.display = 'grid';
        }
        const moodboardContent = document.querySelector('#moodboard-content');
        if (moodboardContent) {
            moodboardContent.style.display = 'none';
        }
    }

    function getSessionData() {
        let datos = {};
        clavesMoodboard.forEach(clave => {
            const valor = sessionStorage.getItem(clave);
            if (valor) {
                try {
                    datos[clave] = JSON.parse(valor);
                } catch (e) {
                    datos[clave] = valor; 
                }
            }
        });
        return datos;
    }

    async function enviarDatosPorCorreo() {
        try {
        const datos = getSessionData();
        datos.idioma = '{{ request.locale.iso_code }}';
        const bodyData = JSON.stringify({
            shop: Shopify.shop,
            datos_completos: datos
        });

        const fetchOptions = {
            method: 'POST',
            headers: {
            'Content-Type': 'application/json',
            },
            body: bodyData,
        };
        console.log('Enviando datos...');
        btnSiguiente.disabled = true;
        const response = await fetch('/apps/upango-b2b/moodboard/send/', fetchOptions);

        if (!response.ok) {
            throw new Error(`Error en la petición: ${response.status} ${response.statusText}`);
        }

        const jsonData = await response.json();
        btnSiguiente.disabled = false;
        return true;

        } catch (error) {
        console.error('Error en fetch:', error);
        alert('Hubo un error al enviar los datos: ' + error.message);
        return false;
        }
    }

    function actualizarBotonSiguiente() {
        const currentStep = getCurrentStep();
        if (!btnSiguiente) return;

        let habilitar = false;
        if (currentStep === 1) {
            const principalChecked = document.querySelectorAll('.principal-color-container .style-checkbox:checked').length > 0;
            const principalInputs = Array.from(document.querySelectorAll('.principal-color-inputs input'))
                .some(input => input.value.trim() !== '');

            const secundaryChecked = document.querySelectorAll('.secundary-color-container .style-checkbox:checked').length > 0;
            const secundaryInputs = Array.from(document.querySelectorAll('.secundary-color-inputs input'))
                .some(input => input.value.trim() !== '');

            habilitar = (principalChecked || principalInputs) && (secundaryChecked || secundaryInputs);
        } 
        else if (currentStep === 8) {
            const nombre = document.getElementById('nombre')?.value.trim();
            const email = document.getElementById('email')?.value.trim();
            const acepta = document.getElementById('AceptarPoliticaPrivacidad')?.checked;

            const emailValido = /^[^\s@]+@[^\s@]+\.[^\s@]+$/.test(email);

            habilitar = !!(nombre && emailValido && acepta);

        } else {
            const tipo = tipos[currentStep];
            if (tipo) {
                habilitar = document.querySelectorAll(`input[name="${tipo}"]:checked`).length > 0;
            }
        }
        btnSiguiente.disabled = !habilitar;
        if (arrowSiguiente) arrowSiguiente.classList.toggle('disabled', !habilitar);
        document.querySelector('.alert-text').style.display = habilitar ? 'none' : 'inherit';
    }

    function verificarSeleccionYHabilitarBoton() {
        const currentStep = getCurrentStep();
        let habilitar = false;
        if (currentStep === 1) {
            const coloresPrimarios = JSON.parse(sessionStorage.getItem('selectedPrincipalColors') || '[]');
            const coloresSecundarios = JSON.parse(sessionStorage.getItem('selectedSecundaryColors') || '[]');
            habilitar = coloresPrimarios.length > 0 && coloresSecundarios.length > 0;
        } else {
            const tipo = tipos[currentStep];
            if (tipo) {
                const valores = JSON.parse(sessionStorage.getItem(tipo) || '[]');
                habilitar = valores.length > 0;
            }
        }

        btnSiguiente.disabled = !habilitar;
        if (arrowSiguiente) arrowSiguiente.classList.toggle('disabled', !habilitar);
        document.querySelector('.alert-text').style.display = habilitar ? 'none' : 'inherit';
    }


    function almacenarValores(currentStep){
        switch (currentStep) {
            case 1: guardarColores(); break;
            case 2: guardarSeleccionado('imagen_proyectar'); break;
            case 3: guardarSeleccionado('uniforme_estaciones'); break;
            case 4: guardarSeleccionado('preferencia_materiales'); break;
            case 5: guardarSeleccionado('estilo_marca'); break;
            case 6: guardarSeleccionado('departamentos_uniformes'); break;
            case 7: guardarSeleccionado('preferencias_estilo'); break;
            case 8: guardarContacto(); break;
        }
    }

    function guardarContacto() {
        const nombre = document.getElementById('nombre').value.trim();
        const email = document.getElementById('email').value.trim();
        const telefono = document.getElementById('telefono').value.trim();
        const datosContacto = {
            nombre: nombre,
            email: email,
            telefono: telefono
        };
        sessionStorage.setItem('datos_contacto', JSON.stringify(datosContacto));
        console.log('Datos de contacto guardados:', datosContacto);
    }


    function guardarSeleccionado(tipo){
        const seleccionados = Array.from(document.querySelectorAll(`input[name="${tipo}"]:checked`))
            .map(input => input.value);
        sessionStorage.setItem(tipo, JSON.stringify(seleccionados));
    }

    function guardarColores(){
        function isVisible(element) {
            return getComputedStyle(element).display !== 'none';
        }
        const principalSelectedColors = Array.from(document.querySelectorAll('.principal-color-container .style-checkbox:checked'))
        .map(input => input.value);
        const principalColorInputs = Array.from(document.querySelectorAll('.principal-color-inputs .field-group'))
        .filter(fieldGroup => {
            const button = fieldGroup.querySelector('.field__button');
            return isVisible(button);
        })
        .map(fieldGroup => fieldGroup.querySelector('input').value);
        const secundarySelectedColors = Array.from(document.querySelectorAll('.secundary-color-container .style-checkbox:checked'))
        .map(input => input.value);
        const secundaryColorInputs = Array.from(document.querySelectorAll('.secundary-color-inputs .field-group'))
        .filter(fieldGroup => {
            const button = fieldGroup.querySelector('.field__button');
            return isVisible(button);
        })
        .map(fieldGroup => fieldGroup.querySelector('input').value);;

        const allPrincipalColors = [...principalSelectedColors, ...principalColorInputs].filter(color => color.trim() !== "");
        const allSecundaryColors = [...secundarySelectedColors, ...secundaryColorInputs].filter(color => color.trim() !== "");

        sessionStorage.setItem('selectedPrincipalColors', JSON.stringify(allPrincipalColors));
        sessionStorage.setItem('selectedSecundaryColors', JSON.stringify(allSecundaryColors));
    }
    
    function loadColorsText(e){
        e.preventDefault();
        const colorInputsContainer = e.target.closest('.color-inputs');
        const newFieldGroup = colorInputsContainer.querySelector('.field-group').cloneNode(true);
        const newInput = newFieldGroup.querySelector('input');
        newInput.value = '';
        const checkIcon = newInput.nextElementSibling;
        checkIcon.style.display = 'none'; 
        newFieldGroup.classList.add('additional_input');
        colorInputsContainer.insertBefore(newFieldGroup, colorInputsContainer.querySelector('.add-color'));
        newInput.addEventListener('input', validateColorInput);
    }

    function loadColors(){
        const colors = [
        "#ECEAE7", "#EDEBE4", "#F6E3D9", "#E7E5D0", "#93C8C5", "#FEB9B6",
        "#DDCE12", "#C4EA10", "#F2ECDC", "#BFCEAD", "#E5D0B7", "#C1D8E2",
